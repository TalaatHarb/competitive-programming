{
  "name": "competitive-programming",
  "version": "1.0.0",
  "description": "Type script solutions for competitive-programming problems from various sites",
  "main": "dist/",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "tsc",
    "build": "run-s lint compile",
    "build:run": "run-s lint compile run",
    "build:test": "run-s lint compile test",
    "lint": "eslint src/**",
    "run": "node dist/src/",
    "test": "node ./node_modules/jasmine/bin/jasmine.js --config=./jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TalaatHarb/competitive-programming.git"
  },
  "keywords": [
    "typescript",
    "competitive-programming"
  ],
  "author": "M. Talaat Harb",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TalaatHarb/competitive-programming/issues"
  },
  "homepage": "https://github.com/TalaatHarb/competitive-programming#readme",
  "devDependencies": {
    "@types/jasmine": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "jasmine": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/node": "^17.0.23"
  }
}
